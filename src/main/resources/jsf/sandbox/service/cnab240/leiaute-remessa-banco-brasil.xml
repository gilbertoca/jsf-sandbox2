                    <Field name="CodigoMovimentoRemessa" padding="ZERO_LEFT" type="INTEGER" length="2" />
                    <Field name="AgenciaMantenedora" padding="ZERO_LEFT" type="INTEGER" length="5" />
                    <Field name="DigitoVerificadorAgencia" type="STRING" length="1" />
                    <Field name="NumeroContaCorrente" padding="ZERO_LEFT" type="INTEGER" length="12" />
