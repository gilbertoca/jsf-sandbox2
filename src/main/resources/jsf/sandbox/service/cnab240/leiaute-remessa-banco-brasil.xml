                    <Field name="AgenciaMantenedora" padding="ZERO_LEFT" type="INTEGER" length="5" />
                    <Field name="DigitoVerificadorAgencia" type="STRING" length="1" />
                    <Field name="NumeroContaCorrente" padding="ZERO_LEFT" type="INTEGER" length="12" />
                    <Field name="DigitoVerificadorContaCorrente" type="STRING" length="1" />
                    <Field name="DigitoVerificadorAgenciaConta" type="STRING" length="1" />
                    <Field name="NomeEmpresa" padding="WHITE_SPACE_RIGHT" type="STRING" length="30" />
                    <Field name="Mensagem1" padding="WHITE_SPACE_RIGHT" type="STRING" length="40" />
                    <Field name="Mensagem2" padding="WHITE_SPACE_RIGHT" type="STRING" length="40" />
                    <Field name="NumeroRemessaRetorno" padding="ZERO_LEFT" type="INTEGER" length="8" />
                    <Field name="DataGravacaoRemessaRetorno" length="8" type="DATE" format="DATE_DDMMYYYY" padding="ZERO_LEFT" />
