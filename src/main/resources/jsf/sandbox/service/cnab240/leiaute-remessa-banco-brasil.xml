                    <Field name="NumeroVersaoLayoutLote" padding="ZERO_LEFT" type="INTEGER" length="3" value="060" />
                    <Field name="CNAB2" type="STRING" length="1" value=" " />
                    <Field name="TipoInscricaoEmpresa" type="INTEGER" length="1" />
                    <Field name="NumeroInscricaoEmpresa" padding="ZERO_LEFT" type="LONG" length="15" />
