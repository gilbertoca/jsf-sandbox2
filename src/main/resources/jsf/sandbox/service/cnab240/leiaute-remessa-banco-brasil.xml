                    <Field name="JurosMoraDia" padding="ZERO_LEFT" type="LONG" length="15" />
                    <Field name="CodigoDesconto1" padding="ZERO_LEFT" type="INTEGER" length="1" />
                    <Field name="DataDesconto1" length="8" type="DATE" format="DATE_DDMMYYYY" padding="ZERO_LEFT" />
                    <Field name="ValorPercentualConcedido" padding="ZERO_LEFT" type="LONG" length="15" />
                    <Field name="ValorIOFRecolhido" padding="ZERO_LEFT" type="LONG" length="15" />
                    <Field name="ValorAbatimento" padding="ZERO_LEFT" type="LONG" length="15" />
                    <Field name="IdentificadorTituloEmpresa" type="STRING" length="25" />
                    <Field name="CodigoProtesto" padding="ZERO_LEFT" type="INTEGER" length="1" />
                    <Field name="NumeroDiasProtesto" padding="ZERO_LEFT" type="INTEGER" length="2" />
                    <Field name="CodigoBaixaDevolucao" padding="ZERO_LEFT" type="INTEGER" length="1" />
                    <Field name="NumeroDiasBaixaDevolucao" padding="ZERO_LEFT" type="INTEGER" length="3" />
                    <Field name="CodigoMoeda" padding="ZERO_LEFT" type="INTEGER" length="2" />
                    <Field name="NumeroContrato" padding="ZERO_LEFT" type="INTEGER" length="10" />
                    <Field name="CNAB2" padding="WHITE_SPACE_RIGHT" type="STRING" length="1" value=" " />
                </GroupOfFields>
                <GroupOfInnerRecords>
                    <!-- SEGMENTO Q -->
                    <Record name="RegistroDetalheSegmentoQ">
                        <GroupOfFields>
                            <!-- Identificador do registro Q -->
                            <IdType name="CodigoSegmentoRegistroDetalhe" value="Q" length="1" position="5" />
                            <Field name="CodigoBancoCompensacao" padding="ZERO_LEFT" type="INTEGER" length="3" value="1" />
